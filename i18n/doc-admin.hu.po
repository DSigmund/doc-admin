"                    ActivityEntityID => 'A4',\n"
"                },\n"
"            },\n"
"        },\n"
"    }\n"
"};\n"
"                    "
msgstr ""
"$Self->{'Process'} = {\n"
"    'F1' => {\n"
"        Name                => 'Könyvrendelés',\n"
"        CreateTime          => '16-02-2012 13:37:00',\n"
"        CreateBy            => '1',\n"
"        ChangeTime          => '17-02-2012 13:37:00',\n"
"        ChangeBy            => '1',\n"
"        State               => 'Active',\n"
"        StartActivity       => 'T1',\n"
"        StartActivityDialog => 'TP1',\n"
"        Path => {\n"
"            'T1' => {\n"
"                'A1' => {\n"
"                    ActivityEntityID => 'T2',\n"
"                },\n"
"            },\n"
