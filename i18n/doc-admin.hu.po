"            StateID => {\n"
"                Type  => 'String',\n"
"                Match => '1',\n"
"            },\n"
"            DynamicField_Marka => {\n"
"                Type  => 'String',\n"
"                Match => 'VW',\n"
"        },\n"
"    },\n"
"    Felt2 => {\n"
"        Type => 'and',\n"
"        Fields => {\n"
"            Queue => {\n"
"                Type  => 'String',\n"
"                Match => 'Nyers',\n"
"            },\n"
"        },\n"
"    },\n"
"},\n"
"                    "

