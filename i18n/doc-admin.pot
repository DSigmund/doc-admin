#: en/content/administration/genericinterface/genericinterface-webservice-gui.xml:1111
msgid "Web service mapping"
msgstr ""

#. type: Content of: <section><section><section><section><para><figure><screenshot>
#: en/content/administration/genericinterface/genericinterface-webservice-gui.xml:1113
msgid ""
"<graphic "
"fileref=\"screenshots/administration/genericinterface/genericinterface-webservice-mapping.png\" "
"scalefit=\"1\" width=\"100%\" contentdepth=\"100%\"></graphic>"
msgstr ""

